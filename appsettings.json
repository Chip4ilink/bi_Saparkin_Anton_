{
  "Mdt": {
    "Database": {
      "Server": "supernout",
      "Port": 1433,
      "Type": "MsSql",
      "Database": "MDT_BI",
      "IntegratedSecurity": true,
      "Login": "",
      "Password": "",
      "AppName": "MDT",
      "CommandTimeout": 30,
      "TransactionTimeout": 300,
      "ConnectionTimeout": 30,
      "MaxPoolSize": 100
    },
    "Application": {
      "InstanceName": "MDT",
      "BaseUrl": "http://localhost/modules/bi/demo/api",
      "BaseUrlUi": "http://localhost/modules/bi/demo",
      "ModulesConfig": {},
      "BasePath": "",
      "DefaultTop": 100,
      "MaxTop": 2000,
      "Version": "",
      "ExtraUrlsUi": []
    },
    "Security": {
      "SecurityDisabled": false,
      "SecurityCacheTime": 15,
      "EtlControllerAdministratorsOnly": true
    },
    "EventPublisher": {
      "Enabled": true,
      "Events": false
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Information",
        "App.Metrics": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] [{SourceContext}] {Message:lj}{NewLine}{Sql}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  }
}